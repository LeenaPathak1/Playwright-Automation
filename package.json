{
  "dependencies": {
    "@cucumber/cucumber": "^12.2.0",
    "exceljs": "^4.4.0",
    "playwright": "^1.54.1"
  },
  "scripts": {
    "CucumberRegressionStagingChrome": "cross-env ENV=staging BROWSER=chrome npx cucumber-js --tags \"@Regression\" --retry 1 --format html:reports/cucumber-report.html --exit",
    "ChromeRegression": " npx playwright test --grep @Regression --reporter=html --retries=1 --project=chrome",
    "CrossBrowserRegression": " npx playwright test --grep @Regression --reporter=html --retries=1"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "@types/node": "^24.0.14",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3"
  }
}
